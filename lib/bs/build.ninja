g_pkg_flg = -bs-package-name my-first-app -bs-ns MyFirstApp
src_root_dir = /home/m2miage/yakine/test/my-first-app
bsc = /home/m2miage/yakine/.nvm/versions/node/v13.2.0/lib/node_modules/bs-platform/linux/bsc.exe
bsdep = /home/m2miage/yakine/.nvm/versions/node/v13.2.0/lib/node_modules/bs-platform/linux/bsb_helper.exe
warnings =  -warn-error +101
bsc_flags = 
ppx_flags = 
g_dpkg_incls = 
g_ns = -bs-ns MyFirstApp
bsb_dir_group = 0
g_lib_incls = -I . -I src
rule build_ast_from_re
  command = $bsc  $warnings -color always $bsc_flags -o $out -bs-syntax-only -bs-binary-ast $in
  description = [34mBuilding[39m [2m${out}[22m
build  src/Demo.reast : build_ast_from_re $src_root_dir/src/Demo.re
rule build_deps
  command = $bsdep -hash dc5f07b8b3a3135b5422fd9fab305ecf $g_ns -g $bsb_dir_group $in
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  src/Demo.d : build_deps src/Demo.reast
rule ml_cmj_cmi
  command = $bsc $g_pkg_flg -color always -bs-suffix $g_lib_incls $warnings $bsc_flags -o $out $in
  dyndep = $in_e.d
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  src/Demo-MyFirstApp.cmj |  src/Demo-MyFirstApp.cmi $src_root_dir/src/Demo.bs.js : ml_cmj_cmi src/Demo.reast ||  src/Demo.d
  g_pkg_flg = $g_pkg_flg  -bs-package-output commonjs:src
rule build_package
  command = $bsc -w -49 -color always -no-alias-deps  $in
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  MyFirstApp.cmi : build_package MyFirstApp.mlmap
